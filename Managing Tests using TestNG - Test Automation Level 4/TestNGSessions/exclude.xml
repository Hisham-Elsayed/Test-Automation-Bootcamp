<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Exclude Broken tests">
    <test name="low priority test cases">
        <groups>
            <run>
                <include name="smoke"/>
                <exclude name="broken"/>
            </run>
        </groups>
        <classes>
            <class name="SessionOne.Groups"></class>
        </classes>
    </test>
</suite>